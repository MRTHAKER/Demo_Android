<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.yudiz.demo">

<uses-permission android:name="android.permission.WAKE_LOCK" />

<uses-feature android:name="android.hardware.type.watch" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Demo">
    <activity android:name="ui.views.Views"/>
    <activity android:name=".UiTopics" />
    <activity android:name="ui.views.ImageView" />
    <activity android:name="ui.views.Switch" />
    <activity android:name="ui.views.ToggleButton" />
    <activity android:name="ui.views.RadioButton" />
    <activity android:name="ui.views.CheckBox" />
    <activity android:name="ui.views.Button" />
    <activity android:name="ui.views.EditText" />
    <activity android:name="ui.views.Textview" />
    <activity android:name="ui.layouts.FrameLayout" />
    <activity android:name="ui.layouts.ConstraintLayout" />
    <activity android:name="ui.layouts.RelativeLayout" />
    <activity android:name="ui.layouts.LinearLayout" />
    <activity android:name="ui.layouts.Layouts" />
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>